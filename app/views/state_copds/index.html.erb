<p id="notice"><%= notice %></p>

<h1>Listing State Copds</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>Respondents</th>
      <th>Has copd</th>
      <th>Percentage</th>
      <th>Ninety five percent confidence interval</th>
      <th>Abbreviation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_copds.each do |state_copd| %>
      <tr>
        <td><%= state_copd.state %></td>
        <td><%= state_copd.respondents %></td>
        <td><%= state_copd.has_copd %></td>
        <td><%= state_copd.percentage %></td>
        <td><%= state_copd.ninety_five_percent_confidence_interval %></td>
        <td><%= state_copd.abbreviation %></td>
        <td><%= link_to 'Show', state_copd %></td>
        <td><%= link_to 'Edit', edit_state_copd_path(state_copd) %></td>
        <td><%= link_to 'Destroy', state_copd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

