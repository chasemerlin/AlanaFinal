<p id="notice"><%= notice %></p>

<h1>Listing Zip Msas</h1>

<table>
  <thead>
    <tr>
      <th>Zip code</th>
      <th>State abbreviation</th>
      <th>Msa number</th>
      <th>Gpci one</th>
      <th>Gpci two</th>
      <th>Gpci three</th>
      <th>County number</th>
      <th>Msa name</th>
      <th>Zip population</th>
      <th>Msa population</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zip_msas.each do |zip_msa| %>
      <tr>
        <td><%= zip_msa.zip_code %></td>
        <td><%= zip_msa.state_abbreviation %></td>
        <td><%= zip_msa.msa_number %></td>
        <td><%= zip_msa.gpci_one %></td>
        <td><%= zip_msa.gpci_two %></td>
        <td><%= zip_msa.gpci_three %></td>
        <td><%= zip_msa.county_number %></td>
        <td><%= zip_msa.msa_name %></td>
        <td><%= zip_msa.zip_population %></td>
        <td><%= zip_msa.msa_population %></td>
        <td><%= link_to 'Show', zip_msa %></td>
        <td><%= link_to 'Edit', edit_zip_msa_path(zip_msa) %></td>
        <td><%= link_to 'Destroy', zip_msa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

