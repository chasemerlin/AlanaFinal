<p id="notice"><%= notice %></p>

<h1>Listing Hospital Generals</h1>

<table>
  <thead>
    <tr>
      <th>Provider</th>
      <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>County</th>
      <th>Phone</th>
      <th>Type</th>
      <th>Ownership</th>
      <th>Emergency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hospital_generals.each do |hospital_general| %>
      <tr>
        <td><%= hospital_general.provider_id %></td>
        <td><%= hospital_general.name %></td>
        <td><%= hospital_general.address %></td>
        <td><%= hospital_general.city %></td>
        <td><%= hospital_general.state %></td>
        <td><%= hospital_general.zip %></td>
        <td><%= hospital_general.county %></td>
        <td><%= hospital_general.phone %></td>
        <td><%= hospital_general.hospital_type %></td>
        <td><%= hospital_general.ownership %></td>
        <td><%= hospital_general.emergency %></td>
        <td><%= link_to 'Show', hospital_general %></td>
        <td><%= link_to 'Edit', edit_hospital_general_path(hospital_general) %></td>
        <td><%= link_to 'Destroy', hospital_general, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hospital general', new_hospital_general_path %>
